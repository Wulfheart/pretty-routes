{
    "name": "wulfheart/pretty_routes",
    "description": "Display your Laravel routes in the console, but make it pretty. ðŸ˜Ž",
    "keywords": [
        "Wulfheart",
        "laravel",
        "pretty_routes"
    ],
    "homepage": "https://github.com/wulfheart/pretty_routes",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Wulf",
            "email": "dev@alexfwulf.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "spatie/phpunit-snapshot-assertions": "^4.2",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Wulfheart\\PrettyRoutes\\": "src",
            "Wulfheart\\PrettyRoutes\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wulfheart\\PrettyRoutes\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wulfheart\\PrettyRoutes\\PrettyRoutesServiceProvider"
            ],
            "aliases": {
                "PrettyRoutes": "Wulfheart\\PrettyRoutes\\PrettyRoutesFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
